{
    "name": "Tabbex - Browser Extension",
    "version": "0.1",
    "description": "Store your favorite tabs permanently.",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "src/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "contextMenus",
        "activeTab"
    ]
}