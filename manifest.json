{
    "name": "Tabbex - Browser Extension",
    "version": "1.0",
    "description": "Store your favorite tabs permanently.",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "src/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "contextMenus",
        "storage",
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "src/popup/popup.html"
    }
}